*.tmx filter=lfs diff=lfs merge=lfs -text
*.tsx filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.ogg filter=lfs diff=lfs merge=lfs -text
*.otf filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text

eva/**/*.png -filter=lfs -diff=lfs -merge=lfs -text
eva/**/*.ogg -filter=lfs -diff=lfs -merge=lfs -text
eva/**/*.ttf -filter=lfs -diff=lfs -merge=lfs -text