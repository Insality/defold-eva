local deftest = require "deftest.deftest"

local tests = {
	-- require("test.tests.smart_value"),
	-- require("test.tests.eva_events"),
	require("test.tests.eva_offers"),
}

local function run_tests()
	for i = 1, #tests do
		deftest.add(tests[i])
	end
	deftest.run()
end

function init(self)
	print('[DefTest]: Init of DefTest')
	run_tests()
end